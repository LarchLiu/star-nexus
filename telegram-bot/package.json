{
  "name": "telegram-bot",
  "type": "module",
  "version": "0.0.1",
  "description": "Manage all your starred pages.",
  "author": "Alex Liu <larch.liu@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/LarchLiu/star-nexus",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LarchLiu/star-nexus.git"
  },
  "bugs": {
    "url": "https://github.com/LarchLiu/star-nexus/issues"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src",
    "build": "zx scripts/build.mjs",
    "debug": "wrangler dev --local",
    "wrangler": "wrangler2",
    "deploy:dist": "wrangler2 publish",
    "deploy:build": "npm run build && wrangler2 publish"
  },
  "dependencies": {
    "@starnexus/core": "workspace:*"
  },
  "devDependencies": {
    "wrangler": "^2.12.0",
    "zx": "^7.2.1"
  }
}

{
  "name": "@stargram/generate-routes",
  "type": "module",
  "version": "0.0.9",
  "description": "Stargram unplugin used to import routes",
  "author": "Alex Liu <larch.liu@gmail.com>",
  "keywords": [
    "unplugin",
    "vite",
    "auto-import",
    "transform"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./*",
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsup src/*.ts --format cjs,esm --dts --splitting --clean",
    "dev": "tsup src/*.ts --format esm --dts --watch src",
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "play": "npm -C ../core run dev",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.7",
    "magic-string": "^0.30.6",
    "scule": "^1.2.0",
    "unimport": "^3.7.1",
    "unplugin": "^1.6.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1"
  }
}

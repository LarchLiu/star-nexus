      "require": "./storage/postgres.cjs"
    },
    "./utils": {
      "types": "./utils.d.ts",
      "import": "./utils.js",
      "require": "./utils.cjs"
    },
    "./webCard": {
      "types": "./webCard.d.ts",
      "import": "./webCard.js",
      "require": "./webCard.cjs"
    },
    "./webInfo": {
      "types": "./webInfo.d.ts",
      "import": "./webInfo.js",
      "require": "./webInfo.cjs"

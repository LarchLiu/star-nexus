{
  "name": "chrome-extension",
  "version": "0.0.0",
  "description": "Manage all your Starred Pages",
  "author": "Alex Liu <larch.liu@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/LarchLiu/star-nexus",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LarchLiu/star-nexus.git"
  },
  "bugs": {
    "url": "https://github.com/LarchLiu/star-nexus/issues"
  },
  "main": "index.js",
  "scripts": {
    "dev": "cross-env CRX_ENV=development run-p dev:*",
    "dev:code": "vite build",
    "dev:content": "vite build --config vite.content.config.ts",
    "dev:settings": "vite build --config vite.settings.config.ts",
    "dev:copy": "node scripts/monitor.js",
    "build": "cross-env CRX_ENV=production pnpm clear && vite build && vite build --config vite.content.config.ts && vite build --config vite.settings.config.ts && node scripts/monitor.js",
    "clear": "rimraf extension",
    "tscheck": "vue-tsc --noEmit",
    "lint": "eslint src"
  },
  "dependencies": {
    "@stargram/core": "workspace:*",
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "element-plus": "^2.3.6",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@types/chrome": "^0.0.237",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.16",
    "@types/ws": "^8.5.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "chokidar": "^3.5.3",
    "chrome-types": "^0.1.199",
    "cross-env": "^7.0.3",
    "eslint-plugin-cypress": "^2.13.3",
    "fs-extra": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.62.1",
    "unocss": "^0.52.7",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.0",
    "vite-plugin-replace": "0.1.1",
    "vue": "^3.3.4",
    "ws": "^8.13.0"
  }
}

{
  "name": "@server/nuxt3",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nuxi build",
    "build:turbo": "turbo run build --filter=@server/nuxt3...",
    "build:netlify": "NITRO_PRESET=netlify-edge turbo run build --filter=@server/nuxt3...",
    "build:cloudflare": "NITRO_PRESET=cloudflare_pages turbo run build --filter=@server/nuxt3...",
    "build:vercel": "turbo run build --filter=@server/nuxt3...",
    "dev": "nuxi dev",
    "dev:cloudflare": "wrangler pages dev dist/",
    "deploy:cloudflare": "wrangler pages publish dist/",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:vercel": "vercel deploy --prod",
    "start": "node .output/server/index.mjs",
    "tscheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "generate": "nuxi generate",
    "start:generate": "npx serve .output/public"
  },
  "dependencies": {
    "@extractus/article-extractor": "v7.2.16",
    "@stargram/core": "workspace:*",
    "@stargram/web-hub": "workspace:*",
    "@supabase/supabase-js": "^2.26.0",
    "@vercel/kv": "^0.2.2",
    "@vue-flow/background": "^1.2.0",
    "@vue-flow/controls": "^1.1.0",
    "@vue-flow/core": "^1.20.2",
    "@vue-flow/minimap": "^1.1.1",
    "langchain": "^0.0.96",
    "satori": "^0.10.1",
    "satori-html": "^0.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@iconify-json/carbon": "^1.1.18",
    "@iconify-json/twemoji": "^1.1.11",
    "@nuxt/devtools": "^0.6.3",
    "@nuxthq/ui": "^2.4.1",
    "@pinia/nuxt": "^0.4.11",
    "@unocss/eslint-config": "^0.53.3",
    "@unocss/nuxt": "^0.53.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vueuse/nuxt": "^10.2.0",
    "eslint": "^8.43.0",
    "netlify-cli": "^15.6.0",
    "nuxt": "^3.6.0",
    "pinia": "^2.1.4",
    "sass": "^1.63.6",
    "turbo": "^1.10.5",
    "unconfig": "^0.3.9",
    "vercel": "^30.2.3",
    "vue-tsc": "^1.8.1",
    "wrangler": "^3.1.1"
  }
}

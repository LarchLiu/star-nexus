{
  "name": "@server/nuxt3",
  "version": "0.0.5",
  "private": true,
  "scripts": {
    "build": "nuxi build",
    "build:turbo": "turbo run build --filter=@server/nuxt3...",
    "build:netlify": "NITRO_PRESET=netlify-edge turbo run build --filter=@server/nuxt3...",
    "build:cl": "NITRO_PRESET=cloudflare turbo run build --filter=@server/nuxt3...",
    "build:vercel": "NITRO_PRESET=vercel-edge turbo run build --filter=@server/nuxt3",
    "dev": "nuxi dev",
    "dev:cl": "wrangler dev .output/server/index.mjs --site .output/public --local",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "publish:cl": "wrangler2 publish",
    "start": "node .output/server/index.mjs",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "postinstall": "nuxi prepare",
    "generate": "nuxi generate",
    "start:generate": "npx serve .output/public"
  },
  "dependencies": {
    "@starnexus/core": "workspace:*",
    "@starnexus/web-hub": "workspace:*",
    "@vercel/kv": "^0.2.1",
    "satori": "^0.8.1",
    "satori-html": "^0.3.2",
    "unfurl.js": "^6.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@iconify-json/carbon": "^1.1.16",
    "@iconify-json/ph": "^1.1.5",
    "@iconify-json/twemoji": "^1.1.11",
    "@nuxt/devtools": "^0.5.0",
    "@nuxtjs/color-mode": "^3.2.0",
    "@pinia/nuxt": "^0.4.11",
    "@unocss/eslint-config": "^0.51.13",
    "@unocss/nuxt": "^0.51.13",
    "@vite-pwa/nuxt": "^0.0.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vueuse/nuxt": "^10.1.2",
    "consola": "^3.1.0",
    "eslint": "^8.40.0",
    "nuxt": "^3.5.0",
    "pinia": "^2.1.2",
    "turbo": "^1.9.8",
    "typescript": "^5.0.4",
    "vue-tsc": "^1.6.5",
    "wrangler": "^2.20.0"
  }
}

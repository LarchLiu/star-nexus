{
  "name": "@server/nuxt3",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nuxi build",
    "build:turbo": "turbo run build --filter=@server/nuxt3...",
    "build:netlify": "NITRO_PRESET=netlify-edge turbo run build --filter=@server/nuxt3...",
    "build:cloudflare": "NITRO_PRESET=cloudflare_pages turbo run build --filter=@server/nuxt3...",
    "build:vercel": "turbo run build --filter=@server/nuxt3...",
    "dev": "nuxi dev",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "dev:cloudflare": "wrangler pages dev dist/",
    "deploy:cloudflare": "wrangler pages publish dist/",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:vercel": "vercel deploy --prod",
    "start": "node .output/server/index.mjs",
    "tscheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "generate": "nuxi generate",
    "start:generate": "npx serve .output/public"
  },
  "dependencies": {
    "@extractus/article-extractor": "^8.0.4",
    "@google/generative-ai": "^0.1.2",
    "@langchain/google-genai": "^0.0.3",
    "@stargram/core": "workspace:*",
    "@stargram/web-hub": "workspace:*",
    "@supabase/supabase-js": "^2.39.0",
    "@vercel/kv": "^1.0.1",
    "@vue-flow/background": "^1.2.0",
    "@vue-flow/controls": "^1.1.0",
    "@vue-flow/core": "^1.27.1",
    "@vue-flow/minimap": "^1.2.0",
    "chromadb": "^1.5.5",
    "crypto-js": "^4.2.0",
    "langchain": "^0.0.208",
    "oauth-1.0a": "^2.2.6",
    "pg": "^8.11.3",
    "query-string": "^8.1.0",
    "satori": "^0.10.11",
    "satori-html": "^0.3.2",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.26",
    "@iconify-json/eos-icons": "^1.1.10",
    "@iconify-json/fad": "^1.1.9",
    "@iconify-json/logos": "^1.1.41",
    "@iconify-json/twemoji": "^1.1.15",
    "@nuxt/devtools": "^1.0.6",
    "@nuxthq/ui": "^2.7.0",
    "@pinia/nuxt": "^0.5.1",
    "@types/web-push": "^3.6.3",
    "@unocss/eslint-config": "^0.58.0",
    "@unocss/nuxt": "^0.58.0",
    "@vite-pwa/nuxt": "^0.4.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vueuse/nuxt": "^10.7.0",
    "netlify-cli": "^17.10.1",
    "nuxt": "^3.8.2",
    "pinia": "^2.1.7",
    "sass": "^1.69.5",
    "turbo": "^1.11.2",
    "unconfig": "^0.3.11",
    "vercel": "^32.7.2",
    "vue-tsc": "^1.8.25",
    "wrangler": "^3.20.0"
  }
}
